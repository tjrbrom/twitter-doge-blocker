{
    "version": 1,
    "rules": [
        {
            "id": "doge_blocker",
            "priority": 1,
            "action": {
                "type": "block"
            },
            "condition": {
                "urlFilter": "*://twitter.com/*",
                "resourceTypes": ["main_frame", "sub_frame"],
                "domains": ["twitter.com"],
                "contains": ["doge"]
            }
        }
    ]
}
